{
  "ApplicationLifetime": {
    "ApplicationStoppingGracePeriodSeconds": 3
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Grpc": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "Quartz": {
    "BooksWarmingJob": {
      "IntervalSeconds": 10
    },
    "PostsWarmingJob": {
      "IntervalSeconds": 10
    }
  },
  "Resilience": {
    "RetryCount": 1,
    "TimeToLiveMilliseconds": 10000
  }
}
