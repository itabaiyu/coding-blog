{
  "AllowedHosts": "*",
  "ApplicationLifetime": {
    "ApplicationShutdownTimeoutSeconds": 45,
    "ApplicationStoppingGracePeriodSeconds": 30
  },
  "Cosmic": {
    "Endpoint": "https://api.cosmicjs.com/v2",
    "BucketSlug": "code-blog"
  },
  "Resilience": {
    "MedianFirstRetryDelayMilliseconds": 50,
    "RetryCount": 5,
    "TimeToLiveMilliseconds": 86400000
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
